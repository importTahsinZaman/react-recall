{
  "name": "react-recall",
  "version": "1.0.3",
  "description": "Debug session recorder for AI-assisted development",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "react-recall": "./dist/server/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server-capture/index.js",
      "types": "./dist/server-capture/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup && echo '\"use client\";' | cat - dist/index.js > temp && mv temp dist/index.js",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/supertest": "^6.0.0",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^2.0.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "debug",
    "devtools",
    "ai",
    "nextjs",
    "react",
    "session-recording",
    "logging"
  ],
  "author": "Tahsin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/importTahsinZaman/react-recall"
  },
  "homepage": "https://github.com/importTahsinZaman/react-recall#readme"
}
